{
    "manifest_version": 3,
    "name": "dERPy AI",
    "description": "Where Enterprise Meets Whimsical Intelligence",
    "version": "1.0.0",
    "content_security_policy": {
      "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/derpyFavicon.png",
        "32": "icons/derpyFavicon.png",
        "48": "icons/derpyFavicon.png",
        "128": "icons/derpyFavicon.png"
      }
    },
    "icons": {
      "16": "icons/derpyFavicon.png",
      "32": "icons/derpyFavicon.png",
      "48": "icons/derpyFavicon.png",
      "128": "icons/derpyFavicon.png"
    }
  }
